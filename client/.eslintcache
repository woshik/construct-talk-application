nts/construct-talk/client/src/components/auth/LoginForm/LoginForm.jsx",["201","202"],"import React from 'react';\nimport './LoginForm.scss';\nimport logo from '../../../assets/image/logo.png';\n\nconst LoginForm = () => (\n  <>\n    <div className=\"title\">\n      <h1>\n        Welcome Back\n        <br />\n        to\n        <br />\n        <img className=\"form-logo\" src={logo} alt=\"logo\" />\n      </h1>\n    </div>\n\n    <div className=\"form-wrapper\">\n      <form method=\"POST\" action=\"#\">\n        <div className=\"form-group\">\n          <label htmlFor=\"username\">Username</label>\n          <input\n            type=\"text\"\n            className=\"form-control\"\n            id=\"username\"\n            autoComplete=\"off\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"password\">Password</label>\n          <input type=\"password\" className=\"form-control\" id=\"password\" />\n        </div>\n        <div className=\"form-button\">\n          <a href=\"./registration.html\" className=\"btn\">\n            Create an Account\n          </a>\n          <button type=\"submit\" className=\"btn\">\n            Login\n          </button>\n        </div>\n      </form>\n    </div>\n\n    <div className=\"footer\">\n      <h2>Connecting Construction Professionals!</h2>\n    </div>\n  </>\n);\n\nexport default LoginForm;\n","/home/w3e/Documents/construct-talk-application/client/src/index.js",[],"/home/w3e/Documents/construct-talk-application/client/src/redux/rootReducer.js",[],"/home/w3e/Documents/construct-talk-application/client/src/redux/registration/index.js",[],["203","204","205","206","207","208"],"/home/w3e/Documents/construct-talk-application/client/src/redux/store.js",[],"/home/w3e/Documents/construct-talk-application/client/src/redux/authentication/index.js",[],"/home/w3e/Documents/construct-talk-application/client/src/pages/Profile.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/BaseRouter.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/pages/Home.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/pages/Registration.jsx",[],["209","210","211","212","213","214"],"/home/w3e/Documents/construct-talk-application/client/src/pages/Login.jsx",[],["215","216","217","218","219","220"],"/home/w3e/Documents/construct-talk-application/client/src/App.jsx",[],["221","222","223","224","225","226"],"/home/w3e/Documents/construct-talk-application/client/src/helper/axios.js",[],"/home/w3e/Documents/construct-talk-application/client/src/assets/data/country.js",[],"/home/w3e/Documents/construct-talk-application/client/src/assets/data/specialty.js",[],"/home/w3e/Documents/construct-talk-application/client/src/validation/auth.js",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Auth/RegistrationForm/RegistrationForm.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Header/Header.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Auth/LoginForm/LoginForm.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Menu/Menu.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Menu/MenuItem.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Logo/Logo.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Logout/Logout.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Base/Base.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Post/Post.jsx",["227"],"/* eslint-disable no-underscore-dangle */\nimport React, { useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { fetchPostData } from '../../redux/post';\n\nconst Post = () => {\n  const postState = useSelector(({ post }) => post);\n  const user = useSelector(({ auth }) => auth.user);\n\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(fetchPostData());\n  }, []);\n\n  return (\n    <>\n      {postState.posts?.length !== 0 ? postState.posts.map((post) => (\n        <div className=\"card mb-5\" key={post._id}>\n          <div className=\"card-header mb-3\">\n            <div className=\"card\">\n              <div className=\"row no-gutters\">\n                <div className=\"profile-picture-wrapper d-flex align-items-start\">\n                  <img\n                    src=\"https://i.pinimg.com/originals/51/f6/fb/51f6fb256629fc755b8870c801092942.png\"\n                    alt=\"avatar\"\n                    className=\"card-img profile-picture\"\n                  />\n                </div>\n                <div className=\"col-10 d-flex align-items-center\">\n                  <div className=\"card-body status\">\n                    <p>\n                      <b className=\"mr-2\">{user.firstName}</b>\n                      {post.image ? 'Added a photo' : ''}\n                    </p>\n                    <p>{post.post}</p>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div className=\"card-body\">\n            <img\n              src={post.image}\n              alt={post.post}\n            />\n          </div>\n        </div>\n      )) : (\n        <h2 className=\"text-center\">Make Some Connection First</h2>\n      )}\n    </>\n  );\n};\n\nexport default Post;\n","/home/w3e/Documents/construct-talk-application/client/src/components/Auth/Base/Base.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/pages/Search.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/pages/Setting.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/SettingBlock/SettingBlock.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/SearchBlock/SearchBlock.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/components/CreatePost/CreatePost.jsx",[],"/home/w3e/Documents/construct-talk-application/client/src/redux/post/index.js",["228"],"import axiosInstance from '../../helper/axios';\n\nconst POST_REQUEST_SENT = 'POST_REQUEST_SENT';\nconst POST_REQUEST_COMPLETE = 'POST_REQUEST_COMPLETE';\nconst FETCH_POST_DATA = 'FETCH_POST_DATA';\nconst UPDATE_PAGE = 'UPDATE_PAGE';\nconst POST_SUCCESS = 'POST_SUCCESS';\nconst POST_FAILURE = 'POST_FAILURE';\nconst CLEAR_ERROR = 'CLEAR_ERROR';\nconst CLEAR_ALL = 'CLEAR_ALL';\n\nconst initialState = {\n  posts: [],\n  success: false,\n  loader: false,\n  error: null,\n  startIndex: 0,\n};\n\nconst POSTReducer = (state = initialState, { type, payload }) => {\n  switch (type) {\n    case POST_REQUEST_SENT:\n      return {\n        ...state,\n        loader: true,\n      };\n    case POST_REQUEST_COMPLETE:\n      return {\n        ...state,\n        loader: false,\n      };\n    case POST_SUCCESS:\n      return {\n        ...state,\n        success: true,\n        error: null,\n      };\n    case POST_FAILURE:\n      return {\n        ...state,\n        success: false,\n        error: payload?.error,\n      };\n    case FETCH_POST_DATA:\n      return {\n        ...state,\n        posts: [...state.posts, ...payload.postList],\n      };\n    case UPDATE_PAGE:\n      return {\n        ...state,\n        startIndex: state.startIndex + payload.index,\n      };\n    case CLEAR_ERROR:\n      return {\n        ...state,\n        error: null,\n      };\n    case CLEAR_ALL:\n      return {\n        ...state,\n        success: false,\n        loader: false,\n        error: null,\n      };\n    default:\n      return {\n        ...state,\n      };\n  }\n};\n\nexport const createPost = (userData) => {\n  function requestSent() {\n    return {\n      type: POST_REQUEST_SENT,\n    };\n  }\n\n  function requestComplete() {\n    return {\n      type: POST_REQUEST_COMPLETE,\n    };\n  }\n\n  function success() {\n    return {\n      type: POST_SUCCESS,\n    };\n  }\n\n  function failure(error) {\n    return {\n      type: POST_FAILURE,\n      payload: { error },\n    };\n  }\n\n  return (dispatch) => {\n    dispatch(requestSent());\n    axiosInstance\n      .post('post', userData, {\n        headers: {\n          'Content-Type': 'multipart/form-data',\n        },\n      })\n      .then(() => {\n        dispatch(success());\n      })\n      .catch((error) => {\n        dispatch(failure(error?.response?.data?.message || 'Server Error'));\n      })\n      .then(() => {\n        dispatch(requestComplete());\n      });\n  };\n};\n\nexport const fetchPostData = () => (dispatch, getState) => {\n  const { post } = getState();\n\n  function completeFetch(postList) {\n    return {\n      type: FETCH_POST_DATA,\n      payload: {\n        postList,\n      },\n    };\n  }\n\n  function updatePage(index) {\n    return {\n      type: UPDATE_PAGE,\n      payload: { index },\n    };\n  }\n\n  axiosInstance\n    .get('post', {\n      params: {\n        start: post.startIndex,\n      },\n    })\n    .then(({ data }) => {\n      const posts = data?.postList ?? [];\n      dispatch(completeFetch(posts));\n      dispatch(updatePage(posts.length));\n    })\n    .catch((error) => console.log(error));\n};\n\nexport const clearError = () => ({\n  type: CLEAR_ERROR,\n});\n\nexport const clearAll = () => ({\n  type: CLEAR_ALL,\n});\n\nexport default POSTReducer;\n","/home/w3e/Documents/construct-talk-application/client/src/validation/post.js",[],"/home/w3e/Documents/construct-talk-application/client/src/components/Loader/Loader.jsx",[],{"ruleId":"229","replacedBy":"230"},{"ruleId":"231","replacedBy":"232"},{"ruleId":"233","replacedBy":"234"},{"ruleId":"235","replacedBy":"236"},{"ruleId":"237","replacedBy":"238"},{"ruleId":"239","replacedBy":"240"},{"ruleId":"241","severity":2,"message":"242","line":20,"column":11,"nodeType":"243","endLine":20,"endColumn":37},{"ruleId":"241","severity":2,"message":"242","line":29,"column":11,"nodeType":"243","endLine":29,